{
	"compilerOptions": {
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"isolatedModules": true,
		"jsx": "react",
		"lib": ["es6", "esnext", "dom"],
		"moduleResolution": "node",
		"noEmit": true,
		"strict": true,
		"target": "esnext",
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"module": "esnext",
		"resolveJsonModule": true,
		"downlevelIteration": true,
		"noImplicitAny": false
	},
	"exclude": [
		"node_modules",
		"babel.config.js",
		"metro.config.js",
		"jest.config.js"
	]
}
